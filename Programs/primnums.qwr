tr. "True\n"
fl. "False\n"
z. 2

is_primnum.:
    $1%1 <false ?false
    $1%2 ?true
    $1%3 ?true
    ecx = 2

loop.:
    div $1 ecx
    .edx%0 ?false

    ecx++
    mul ecx ecx
    .$1%eax ?loop <loop


true.:
    print tr
    /

false.:
    print fl
    /

msg. "Number: "
input. #128
prompt msg input
num. 0
atoi num input
+#(num) @is_primnum
