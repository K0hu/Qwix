tr. "True\n"
fl. "False\n"
z. 2

is_primnum.:
    $1%1 <is_false ?is_false
    $1%2 ?is_true
    $1%3 ?is_true
    ecx = 2

prim_loop.:
    div $1 ecx
    .edx%0 ?is_false

    ecx++
    mul ecx ecx
    .$1%eax ?prim_loop <prim_loop


is_true.:
    print tr
    /

is_false.:
    print fl
    /

msg. "Number: "
input. #128
prompt msg input
num. 0
atoi num input
+#(num) @is_primnum
